{
  "name": "hanse-kim.dev",
  "version": "1.0.0",
  "private": true,
  "description": "hanse-kim.dev",
  "author": "Hanse Kim",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "pull-posts": "rm -rf .posts && git clone https://github.com/hanse-kim/hanse-kim.github.io-posts .posts",
    "remove-hided": "sh .scripts/remove-hided.sh .posts",
    "deploy": "yarn pull-posts && yarn remove-hided && gatsby build && touch public/.nojekyll && gh-pages -d public -b deploy --dotfiles"
  },
  "dependencies": {
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.3.31",
    "gatsby": "^5.13.5",
    "gatsby-plugin-google-gtag": "^5.13.1",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-root-import": "^2.0.9",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-remark-prismjs": "^7.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "gh-pages": "^6.1.1",
    "kebab-case": "^2.0.1",
    "postcss": "^8.4.41",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.10",
    "tw-colors": "^3.3.1",
    "typescript": "^5.3.3"
  }
}
